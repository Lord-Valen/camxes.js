{
  "name": "camxes.js",
  "version": "0.0.1",
  "description": "A Lojban parser.",
  "author": "Lord-Valen",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "type": "module",
  "packageManager": "pnpm@*",
  "main": "dist/index.cjs",
  "exports": {
    "require": "dist/index.cjs"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "cjs"
    ],
    "dts": true,
    "clean": true,
    "publicDir": true
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint src",
    "test": "vitest --run",
    "dev": "vitest"
  },
  "devDependencies": {
    "@eslint/js": "^8",
    "@types/node": "^18.11.13",
    "eslint": "^8.57.0",
    "tsup": "^8.0.2",
    "turbo": "^1.13.3",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.10.0",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "pegjs": "^0.7.0"
  }
}
